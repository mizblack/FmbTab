<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowActionBarOverlay">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="MaterialTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="AppCompatNoTitle.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppCompatNoTitle.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />


    <style name="MainMenuButton">
        <item name="android:gravity">center</item>
        <item name="android:layout_width">156dp</item>
        <item name="android:layout_height">156dp</item>
        <item name="android:layout_margin">8dp</item>

    </style>

    <style name="MainRankOrHoleOrPuttOrMeterTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MainColumnHoleNumberTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>"
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MainColumnParOrMetersTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:text">-</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="MainColumnInOrOutTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>"
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:textColor">@color/lightGray</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="MainHoleTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingTop">16dp</item>
        <item name="android:text">-</item>
        <item name="android:textColor">@color/lightGray</item>
        <item name="android:textSize">36sp</item>
        <item name="android:gravity">start</item>
        <item name="android:paddingStart">16dp</item>
    </style>

    <style name="MainPuttTextView">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:text">-</item>
        <item name="android:textColor">@color/lightGray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:gravity">start</item>
        <item name="android:paddingStart">16dp</item>
    </style>

    <style name="MainTabTitleTextViewBold">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:text">-</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize" tools:ignore="SpUsage">24sp</item>
    </style>
    <style name="MainTabTitleTextViewNormal">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:text">-</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize" tools:ignore="SpUsage">22sp</item>
    </style>
    <style name="MainTabTitleTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:text">-</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize" tools:ignore="SpUsage">22sp</item>
    </style>

    <style name="MainTabTitleTextViewPadding" parent="MainTabTitleTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:text">-</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize" tools:ignore="SpUsage">22sp</item>
        <item name="android:paddingEnd">60dp</item>
    </style>

    <style name="ShadeTabTitleTextView" parent="MainTabTitleTextView">
        <item name="android:layout_marginStart">16dp</item>
        <item name="android:paddingStart">20dp</item>
        <item name="android:paddingEnd">20dp</item>
    </style>

    <style name="ShadeGuestNameTextView">
        <item name="android:layout_width">80dp</item>
        <item name="android:layout_height">80dp</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize" tools:ignore="SpUsage">20dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="GalleryGuestTextView">
        <item name="android:gravity">center</item>
        <item name="android:textSize" tools:ignore="SpUsage">22dp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="RankColumnTextView">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">70dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ScoreInserterTextView">

        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center</item>
<!--        <item name="android:textStyle">bold</item>-->
        <item name="android:background">@drawable/shape_gray_edge</item>
    </style>

    <style name="InserterTextView">

        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">20sp</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="SubMenuTextView">
        <item name="android:background">@color/ebonyBlack</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">23sp</item>
        <item name="android:gravity">left</item>
    </style>

    <style name="InserterTextViewBold">

        <item name="android:textColor">@color/black</item>
        <item name="android:textSize">24sp</item>
        <item name="android:gravity">center</item>
        <item name="android:textStyle">bold</item>
    </style>

    <!--   //*************-->
    <style name="RankColumnHoleTextView">
        <item name="android:layout_width">@dimen/ranking_linear_width</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">18sp</item>
        <item name="android:text">-</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="RankRowTextView">
        <item name="android:layout_width">100dp</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:text">-</item>
        <item name="android:textSize" tools:ignore="SpUsage">18dp</item>
        <item name="android:gravity">center</item>
    </style>

    <style name="ItemOrderBillTextStyle">
        <item name="android:layout_width">230dp</item>
        <item name="android:layout_height">25dp</item>
        <item name="android:textColor">@color/gray</item>
        <item name="android:text">-</item>
        <item name="android:textSize">13sp</item>
        <item name="android:gravity">center_vertical</item>
    </style>
    <style name="GolfClubListTextStyle">
        <item name="android:textColor">@color/gray</item>
        <item name="android:textSize">22sp</item>
        <item name="android:gravity">center</item>
    </style>

</resources>
