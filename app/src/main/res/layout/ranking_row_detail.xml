            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="4" />

        <TextView
            android:id="@+id/tv_endHole5"
            style="@style/GlobalTextView.18SP.ebonyBlack.NotoSans.Medium"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="5" />

        <TextView
            android:id="@+id/tv_endHole6"
            style="@style/GlobalTextView.18SP.ebonyBlack.NotoSans.Medium"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="6" />

        <TextView
            android:id="@+id/tv_endHole7"
            style="@style/GlobalTextView.18SP.ebonyBlack.NotoSans.Medium"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="7" />

        <TextView
            android:id="@+id/tv_endHole8"
            style="@style/GlobalTextView.18SP.ebonyBlack.NotoSans.Medium"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="8" />

        <TextView
            android:id="@+id/tv_endHole9"
            style="@style/GlobalTextView.18SP.ebonyBlack.NotoSans.Medium"
            android:layout_width="40dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="9" />

        <View
            android:layout_width="1dp"
            android:layout_height="match_parent"
            android:background="@color/zumthor" />


        <TextView
            android:id="@+id/tv_endHole"
            style="@style/GlobalTextView.18SP.ebonyBlack.NotoSans.Medium"
            android:layout_width="50dp"
            android:layout_height="match_parent"
            android:gravity="center"
            android:text="In" />
    </LinearLayout>

</layout>