<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <View
        android:id="@+id/divider00"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        android:visibility="gone" />

    <LinearLayout
        android:id="@+id/menuLinearLayout"
        android:layout_width="match_parent"
        android:layout_height="60dp"
        android:orientation="horizontal"
        android:visibility="visible">

        <TextView
            android:id="@+id/menuNameTextView"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginStart="20dp"
            android:layout_weight="1"
            android:gravity="center_vertical"
            android:text="-"
            android:textColor="@color/gray"
            android:textSize="18dp"
            tools:ignore="HardcodedText,SpUsage" />

        <TextView
            android:id="@+id/menuQuantityTextView"
            android:layout_width="wrap_content"
            android:layout_height="match_parent"
            android:gravity="center_vertical"
            android:text="-개"
            android:textColor="@color/gray"
            android:textSize="18dp"
            tools:ignore="HardcodedText,SpUsage" />

        <View
            android:layout_width="20dp"
            android:layout_height="match_parent" />

        <LinearLayout
            android:id="@+id/extendAndFoldingLinearLayout"
            android:layout_width="60dp"
            android:layout_height="60dp"
            android:gravity="center">

            <ImageView
                android:id="@+id/extendAndFoldingImageView"
                android:layout_width="20dp"
                android:layout_height="20dp"
                android:src="@drawable/down_arrow"
                tools:ignore="ContentDescription" />

        </LinearLayout>


    </LinearLayout>

    <LinearLayout
        android:id="@+id/memberOrderLinearLayout"
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <LinearLayout
            android:id="@+id/member00Order"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/member00NameTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="-"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <TextView
                android:id="@+id/member00MenuQuantityTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="-개"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,MissingPrefix,SpUsage" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <LinearLayout
                android:id="@+id/deleteLinearLayout00"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/red_x"
                    tools:ignore="ContentDescription" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/member01Order"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/member01NameTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="-"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <TextView
                android:id="@+id/member01MenuQuantityTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="-개"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <LinearLayout
                android:id="@+id/deleteLinearLayout01"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/red_x"
                    tools:ignore="ContentDescription" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/member02Order"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/member02NameTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="-"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <TextView
                android:id="@+id/member02MenuQuantityTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="-개"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <LinearLayout
                android:id="@+id/deleteLinearLayout02"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/red_x"
                    tools:ignore="ContentDescription" />

            </LinearLayout>

        </LinearLayout>

        <LinearLayout
            android:id="@+id/member03Order"
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:orientation="horizontal"
            android:visibility="gone">

            <TextView
                android:id="@+id/member03NameTextView"
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_marginStart="20dp"
                android:layout_weight="1"
                android:gravity="center_vertical"
                android:text="-"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <TextView
                android:id="@+id/member03MenuQuantityTextView"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:text="-개"
                android:textColor="@color/gray"
                android:textSize="18dp"
                tools:ignore="HardcodedText,SpUsage" />

            <View
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1" />

            <LinearLayout
                android:id="@+id/deleteLinearLayout03"
                android:layout_width="60dp"
                android:layout_height="60dp"
                android:gravity="center">

                <ImageView
                    android:layout_width="20dp"
                    android:layout_height="20dp"
                    android:src="@drawable/red_x"
                    tools:ignore="ContentDescription" />

            </LinearLayout>

        </LinearLayout>

    </LinearLayout>

    <View
        android:id="@+id/divider01"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="@color/black"
        android:visibility="gone" />

</LinearLayout>